<div class="edit-project-profile-div">

  <div class="corner-heading">
    <h4>Editing Project Profile
    <%= link_to 'Done Editing', project_details_path(@project_profile), :class=>'btn' %></h4>
  </div>

  <div class="scroll-splitter"></div>
	
	<% @details.sort.map do |key,entry| %>
    <%= render partial: 'project_details_entry/editable_entry', locals: {key: key, entry: entry} %>
  <% end %>
	
  <p>
  <%= link_to 'New Section', new_project_details_entry_path(@project_profile), class: 'btn' %>
  </p>

<p>
<%= link_to 'Back', project_details_path(@project_profile) %>
</p>


</div>
