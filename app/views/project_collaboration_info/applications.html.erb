<table class="table table-striped table-bordered box">
  <thead>
    <tr>
      <th>Status</th>
      <th>User</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @project.applications.includes({applicant: :profile}).each do |application| %>
      <tr>
        <td><%= application.status %></td>
        <td><%= link_to application.applicant.username, application.applicant.profile %></td>
        <td><%= link_to 'Details', application %></td>
      </tr>
    <% end %>
  </tbody>
</table>
