<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Project</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    <% @user.owned_projects.each do |project| %>
      <tr>
        <td><%= link_to project.name, project.profile %></td>
        <td><%= project.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Project', new_project_path, class: 'btn btn-inverse' %>
